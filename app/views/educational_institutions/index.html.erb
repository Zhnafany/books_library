<h1>Listing educational_institutions</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @educational_institutions.each do |educational_institution| %>
  <tr>
    <td><%=h educational_institution.name %></td>
    <td><%= link_to 'Show', educational_institution %></td>
    <td><%= link_to 'Edit', edit_educational_institution_path(educational_institution) %></td>
    <td><%= link_to 'Destroy', educational_institution, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New educational_institution', new_educational_institution_path %>